
class ClientDrucker extends Thread{
  
  Socket socket;
  ClientDrucker(Socket s){
    socket=s;
    }
  
  public void run(){
    try {
      while (true) { 
        System.out.println(this.socket.readLine());
      } // end of while
      
    } catch(Exception e) {
      
    } finally {
      
    } // end of try
    }
  
  
  
  }
